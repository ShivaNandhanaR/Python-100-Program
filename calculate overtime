ovpay=0
sum=0
for i in range(1,11):
    print("enter working hours of emp",i,":")
    h=int(input())
    if(h>40):
        extra=h-40
        ovpay=extra*12
        print("over time pay of emp",i,"is",ovpay)
        sum=sum+ovpay
    else:
        print("no overtime pay")
        print("total overtime of all emloyees:",sum)ov


